#set xor;
symbols f1/2,f2/2,f5/2,pair/2,fst/1,snd/1,macFail/0,syncFail/0,reject/0;

channels C;
private sqnSN,sqnSN1,sqnSN2,k,kZ,rand,rand1,rand2;

var X,Y,Z,X1,X2,
	xrand,msg,xmac,ak,xsqn,mac,res,ck,ik,
	xrand1,msg1,xmac1,ak1,xsqn1,mac1,res1,ck1,ik1,
	xrand2,msg2,xmac2,ak2,xsqn2,mac2,res2,ck2,ik2,
	akSN,akSN1,akSN2,
	macSN,macSN1,macSN2,
	autnSN,autnSN1,autnSN2,
	xresSN,xresSN1,xresSN2,
	resSN,resSN1,resSN2;

rewrite fst(pair(X, Y)) -> X;
rewrite snd(pair(X, Y)) -> Y;

akaMS1 = 
	in(C,X1).
	let xrand1 = fst(X1) in
	let msg1 = fst(snd(X1)) in
	let xmac1 = snd(snd(X1)) in
	let ak1 = f5(kZ,xrand1) in
	let xsqn1 = ak1 + msg1 in
	let mac1 = f1(kZ,pair(xrand1,xsqn1)) in
	if xmac1 != mac1 then
		out(C,macFail)
	else
		out(C,f2(kZ,xrand1));


akaMS2 = 
	in(C,X2).
	let xrand2 = fst(X2) in
	let msg2 = fst(snd(X2)) in
	let xmac2 = snd(snd(X2)) in
	let ak2 = f5(k,xrand2) in
	let xsqn2 = ak2 + msg2 in
	let mac2 = f1(k,pair(xrand2,xsqn2)) in
	if xmac2 != mac2 then
		out(C,macFail)
	else
		out(C,f2(k,xrand2));

akaMS = 
	in(C,X).
	let xrand= fst(X) in
	let msg = fst(snd(X)) in
	let xmac = snd(snd(X)) in
	let ak = f5(k,xrand) in
	let xsqn = ak + msg in
	let mac = f1(k,pair(xrand,xsqn)) in
	if xmac != mac then
		out(C,macFail) ::
		akaMS2
	else
		out(C,f2(k,xrand)).
		
	in(C,X2).
	let xrand2 = fst(X2) in
	let msg2 = fst(snd(X2)) in
	let xmac2 = snd(snd(X2)) in
	let ak2 = f5(k,xrand2) in
	let xsqn2 = ak2 + msg2 in
	let mac2 = f1(k,pair(xrand2,xsqn2)) in
	if xmac2 != mac2 then
		out(C,macFail)
	else
		if xsqn2 = xsqn
		then
			out(C,syncFail)
		else
			out(C,f2(k,xrand2));
		
akaSN1 = 
	let akSN1 = f5(k,rand1) in
	let macSN1 = f1(k,pair(rand1,sqnSN1)) in
	let autnSN1 = pair(sqnSN1 + akSN1, macSN1) in
	out(C,pair(rand1,autnSN1)).
	in(C,xresSN1).
	let resSN1 = f2(k,rand1) in
	if xresSN1 != resSN1
	then
		out(C,reject)
	else
		0;
		
akaSN2 = 
	let akSN2 = f5(k,rand2) in
	let macSN2 = f1(k,pair(rand2,sqnSN2)) in
	let autnSN2 = pair(sqnSN2 + akSN2, macSN2) in
	out(C,pair(rand2,autnSN2)).
	in(C,xresSN2).
	let resSN2 = f2(k,rand2) in
	if xresSN2 != resSN2
	then
		out(C,reject)
	else 0;

akaSN = 
	let akSN = f5(k,rand) in
	let macSN = f1(k,pair(rand,sqnSN)) in
	let autnSN = pair(sqnSN + akSN, macSN) in
	out(C,pair(rand,autnSN)).
	in(C,xresSN).
	let resSN = f2(k,rand) in
	if xresSN != resSN
	then
		out(C,reject) ::
		akaSN2
	else
		
	let akSN2 = f5(k,rand2) in
	let macSN2 = f1(k,pair(rand2,sqnSN2)) in
	let autnSN2 = pair(sqnSN2 + akSN2, macSN2) in
	out(C,pair(rand2,autnSN2)).
	in(C,xresSN2).
	let resSN2 = f2(k,rand2) in
	if xresSN2 != resSN2
	then
		out(C,reject)
	else 0;
	
		


		

 system1  = 
	akaMS || akaSN;

 system2  = (akaMS1 :: akaMS2 ) || (akaSN1 :: akaSN2);



equivalentct? system1 and system2 ; 
